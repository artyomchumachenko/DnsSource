<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainAnchorPane" prefHeight="210.0" prefWidth="398.0" snapToPixel="false" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.mai.coursework.dns.controller.ProfileController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
   <children>
      <AnchorPane layoutX="95.0" prefHeight="61.0" prefWidth="200.0">
         <children>
            <Label layoutX="58.0" layoutY="19.0" text="Профиль">
               <font>
                  <Font name="Verdana" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <VBox layoutX="33.0" layoutY="84.0" prefHeight="97.0" prefWidth="325.0" spacing="20.0">
         <children>
            <TextField fx:id="loginTextField" editable="false" focusTraversable="false" mouseTransparent="true" prefHeight="34.0" prefWidth="326.0" promptText="Логин">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <TextField fx:id="phoneTextField" editable="false" focusTraversable="false" mouseTransparent="true" prefHeight="34.0" prefWidth="326.0" promptText="Номер телефона">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
         </children>
      </VBox>
      <Button fx:id="exit" layoutX="154.0" layoutY="196.0" mnemonicParsing="false" onMouseClicked="#exitFromProfile" prefHeight="34.0" prefWidth="82.0" text="Выход" />
   </children>
</AnchorPane>
