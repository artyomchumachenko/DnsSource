    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
   <children>
      <AnchorPane layoutX="95.0" prefHeight="61.0" prefWidth="200.0">
         <children>
            <Label layoutX="64.0" layoutY="12.0" text="Вход">
               <font>
                  <Font name="Verdana" size="29.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <VBox layoutX="33.0" layoutY="84.0" prefHeight="97.0" prefWidth="325.0" spacing="20.0">
         <children>
            <TextField fx:id="loginTextField" prefHeight="34.0" prefWidth="326.0" promptText="Логин">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <PasswordField fx:id="passwordTextField" prefHeight="34.0" prefWidth="326.0" promptText="Пароль">
               <font>
                  <Font size="15.0" />
               </font>
            </PasswordField>
